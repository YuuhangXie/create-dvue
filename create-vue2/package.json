{
  "name": "create-dvue",
  "version": "2.0.0",
  "description": "An easy way to start a Vue mutli-page project",
  "type": "module",
  "bin": {
    "create-dvue": "outfile.cjs"
  },
  "files": [
    "outfile.cjs",
    "template"
  ],
  "engines": {
    "node": "^14.13.1 || >=16.0.0"
  },
  "scripts": {
    "prepare": "husky install",
    "format": "prettier --write .",
    "build": "zx ./scripts/build.mjs",
    "snapshot": "zx ./scripts/snapshot.mjs",
    "pretest": "run-s build snapshot",
    "test": "zx ./scripts/test.mjs",
    "prepublishOnly": "zx ./scripts/prepublish.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuejs/create-vue.git"
  },
  "keywords": [],
  "author": "Yuhang Xie <xieyuhang@didiglobal.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/YuuhangXie/create-dvue/issues"
  },
  "homepage": "https://github.com/YuuhangXie/create-dvue",
  "devDependencies": {
    "@types/eslint": "^8.4.5",
    "@types/prompts": "^2.0.14",
    "@vue/tsconfig": "^0.1.3",
    "esbuild": "^0.14.49",
    "esbuild-plugin-license": "^1.2.2",
    "husky": "^8.0.1",
    "kolorist": "^1.5.1",
    "lint-staged": "^13.0.3",
    "minimist": "^1.2.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "prompts": "^2.4.2",
    "zx": "^4.3.0"
  },
  "lint-staged": {
    "*.{js,ts,vue,json}": [
      "prettier --write"
    ]
  }
}
